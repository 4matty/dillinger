<ul class="dropdown dropdown-menu" role="menu">
  <li>
    <a href="#" class="import-dropbox unlinked">
      <span>Dropbox</span>
      <span class="icon icon-link"><%- include ../icons/linked %></span>
    </a>
  </li>

  <% if (isGithubConfigured) { %>
  <li ng-controller="Github as github">
    <a href="#"
       class="import-github <%= isGithubAuth ? 'linked' : 'unlinked' %>"
       ng-click="github.importFile('<%= github_username %>')">
      <span>Github</span>
      <span class="icon icon-link"><%- include ../icons/linked %></span>
    </a>
  </li>
  <% } %>

  <li>
    <a href="#" class="import-google-drive unlinked">
      <span>Google Drive</span>
      <span class="icon icon-link"><%- include ../icons/linked %></span>
    </a>
  </li>
  <li>
    <a href="#" class="import-one-drive unlinked">
      <span>One Drive</span>
      <span class="icon icon-link"><%- include ../icons/linked %></span>
    </a>
  </li>
</ul>
